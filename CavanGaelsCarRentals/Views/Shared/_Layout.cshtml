<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title - My ASP.NET MVC Application</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
        @Styles.Render("~/Content/themes/base/css", "~/Content/css", "~/Content/styles.css")
        @Scripts.Render("~/bundles/modernizr")
        @Scripts.Render("~/bundles/jquery")
        <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.21/jquery-ui.min.js"></script>
    </head>
    <body>


    <div id="mainbody">
<div id="content">


<div id="header">

<a href="#"><img src="images/logo.jpg" alt="Cavan Gaels" width="221" height="47" border="0" /></a>

<ul>
<li class="home">@Html.ActionLink("Home", "Index", "Home")</li>
<li class="rent"> @Html.ActionLink("New Booking","Create", "Booking")</li>
<li class="reservation"><a href="#" title="Reservation">Reservation</a></li>
<li class="vehicles"><a href="#" title="Vehicles">Vehicles</a></li>
<li class="contacts">@Html.ActionLink("Contact", "Contact", "Home")</li>

<li class="login / register"><a href="#" title="Login / Register">Login / Register</a></li>
</ul>
</div>

<div id="maincontent">
<div id="inmaincontent">


            @RenderSection("featured", required: false)
                @RenderBody()
                </div>

</div>
 
</div>

        <footer>


        <div id="footer">

<img src="images/call.jpg" alt="" width="233" height="76" />

<p>@Html.ActionLink("Home", "Index", "Home") | <a href="#">ABOUT US</a> | <a href="#">RENT A CAR</a> | <a href="#">VEHICLES</a> | <a href="#">PRICES</a> | <a href="#">SUPPORT INFO</a> | <a href="#">CONTACTS</a> | @Html.ActionLink("CSV Upload", "Index", "Supplier")<br/>
Copyright &copy; Cavan Gaels.<a></a></p>
</div>
</div>
</div>

        </footer>

        @RenderSection("scripts", required: false)
    </body>
</html>
